{
    "name": "Custom Dev Functions",
    "version": "1",
    "manifest_version": 2,
    "description": "Injects script tag for custom dev tool functionality",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_title": "custom dev"
    },
    "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs",
      "webNavigation"
    ]
  }