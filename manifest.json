{
    "name": "Custom Dev Functions",
    "version": "1",
    "manifest_version": 2,
    "description": "Injects script tag for custom dev tool functionality",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_title": "custom dev"
    },
    "permissions": [
      "*://www.simplertrading.com/*",
      "*://dev.*",
      "http://127.0.0.1/*",
      "https://127.0.0.1/*",
      "tabs",
      "webNavigation"
    ]
  }